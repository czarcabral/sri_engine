RULE GrandFather($X,$Y):- AND Father($X,$Z) Parent($Z,$Y)
RULE GrandFathersFather($X,$Y):- AND Father($X,$A) Father($A,$B) Parent($B,$Y)
RULE GrandMother($X,$Y):- AND Mother($X,$Z) Mother($Z,$Y)
RULE GrandMother($X,$Y):- AND Mother($X,$Z) Father($Z,$Y)
RULE Parent($X,$Y):- OR Father($X,$Y) Mother($X,$Y)
RULE Parent2($X,$Y):- OR Father($X,$Y) Parent($X,$Y)
RULE rule1($a,$b,$c):- OR R1($a,$b,$c) R2($a,$b,$c) R3($a,$b,$c)
RULE rule2($a,$b,$c):- OR R1($a,$b,$c) R4($a,$b,$c,$d,$e) R5($a,$b,$c,$d)
