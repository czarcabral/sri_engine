RULE rule4($b,$d):- OR R2($a,$b,$d) R3($a,$b,$c)
RULE rule5($b,$d):- OR R2($a,$c,$d) R3($a,$b,$c) R4($a,$b,$d,$e,$f)
RULE rule6($a,$b):- OR R1($b,$c,$a) R5($a,$b,$c,$d)
RULE rule7($a,$b,$c):- AND R6($a,$b) R7($b,$c) R8($b,$d) R9($c,$d)
